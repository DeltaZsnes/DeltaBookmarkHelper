{
	"name": "Delta Bookmark Helper",
	"version": "0.2",
	"description": "A simple bookmark helper for your Chrome Browser",
	"permissions": [
		"activeTab",
		"storage",
		"bookmarks"
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "Delta Bookmark Helper",
		"default_icon": {
			"16": "images/icon16.png",
			"32": "images/icon32.png",
			"48": "images/icon48.png",
			"128": "images/icon128.png"
		}
	},
	"icons": {
		"16": "images/icon16.png",
		"32": "images/icon32.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"manifest_version": 2
}
